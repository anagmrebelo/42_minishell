- shell level
	leaks
	echo $SHLVL\n
	[EXPORTS SHLVL=' 2000000'] echo $SHLVL\n                                                                                                                              [FAIL]
	[EXPORTS SHLVL=' -2000000'] echo $SHLVL\n                                                                                                                             [FAIL]
	[EXPORTS SHLVL=' 2147483647'] echo $SHLVL\n                                                                                                                           [FAIL]
	[EXPORTS SHLVL=' -2147483648'] echo $SHLVL\n
	[EXPORTS SHLVL=' -2147483649'] echo $SHLVL\n
	[EXPORTS SHLVL=' -3147483649'] echo $SHLVL\n
	[EXPORTS SHLVL=' 9223372036854775808'] echo $SHLVL\n                                                                                                                  [FAIL]
	[EXPORTS SHLVL=' 18446744073709551615'] echo $SHLVL\n                                                                                                                 [FAIL]
	[EXPORTS SHLVL=' 18446744073709551616'] echo $SHLVL\n
	[EXPORTS SHLVL=' +2000000'] echo $SHLVL\n                                                                                                                             [FAIL]
	[EXPORTS SHLVL=' +2147483647'] echo $SHLVL\n
	[EXPORTS SHLVL=' 0123456789'] echo $SHLVL\n                                                                                                                           [FAIL]
	[EXPORTS SHLVL=' -0123456789'] echo $SHLVL\n
	[EXPORTS SHLVL=' 0123456789'] echo $SHLVL\n                                                                                                                           [FAIL]
	[EXPORTS SHLVL=' -0123456789'] echo $SHLVL\n
	

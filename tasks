- signals
- exit
	exit numeros > 255
	# exit 3 | exit 5 (no imprimir exit por pantalla)
- unset
	[EXPORTS A='a' B='b' C='c'] unset A 'asdf ' B ' asdf asdf asd f' ' asdf ' ... [FAIL]
	[EXPORTS A='a' B='b' C='c'] unset 'asdf ' B ' asdf asdf asd f' ' asdf ' ''... [FAIL]
	[EXPORTS A='a' B='b' C='c'] unset A 'asdf ' B ' asdf asdf asd f' ' asdf ' ... [FAIL]                                                           

- export spaces (NON PRIORITARY)
	[EXPORTS PATH='        '] echo $PATH\n                                                                                                                                [FAIL]
	[EXPORTS PATH='   :    '] echo $PATH\n                                                                                                                                [FAIL]
	[EXPORTS PATH='     /bin      '] echo $PATH\n                                                                                                                         [FAIL]
	[EXPORTS PATH='     /sbin      '] echo $PATH\n

-interpolation (NON PRIORITARY)
	[EXPORTS A='foo ' B=' bar  ' C='baz '] echo $A"$B"$C"A"$B"$C"\n                                                                                                       [FAIL]
	[EXPORTS A='foo ' B=' bar  ' C='baz '] echo $A'$B'$C'A'$B'$C'\n
	[EXPORTS A='    '] echo [$A]\n
	[EXPORTS A=' \  '] echo [$A]\n
	[SETUP touch a b c] [EXPORTS LS='ls -l'] $LS\n

- redirections
	ls | > redirect
